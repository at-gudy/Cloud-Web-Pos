{
  "name": "cloud-web-pos-back",
  "version": "1.0.0",
  "description": "",
  "main": "server.js",
  "scripts": {
    "start": "npm run serve",
    "debug": "npm run build && npm run watch-debug",
    "build": "npm run build-sass && npm run build-ts && npm run tslint && npm run copy-static-assets",
    "clean": "rm -rf dist",
    "serve": "npm run build && node dist/server.js",
    "tslint": "tslint -c tslint.json -p tsconfig.json",
    "build-ts": "tsc",
    "watch-ts": "tsc -w",
    "build-sass": "node-sass src/public/css/main.scss dist/public/css/main.css",
    "watch-sass": "node-sass -w src/public/css/main.scss dist/public/css/main.css",
    "serve-debug": "nodemon --inspect dist/server.js -e js,hbs",
    "watch-debug": "concurrently -k -p \"[{name}]\" -n \"TypeScript,Node\" -c \"yellow.bold,cyan.bold,green.bold\" \"npm run watch-sass\" \"npm run watch-ts\" \"npm run serve-debug\"",
    "copy-static-assets": "ts-node tools/copyStaticAssets.ts"
  },
  "author": "winetree94",
  "license": "MIT",
  "dependencies": {
    "body-parser": "^1.19.0",
    "connect-mongo": "^3.0.0",
    "dotenv": "^8.0.0",
    "express": "^4.17.1",
    "hbs": "^4.0.4",
    "mongoose": "^5.6.2",
    "winston": "^3.2.1"
  },
  "devDependencies": {
    "@types/body-parser": "^1.17.0",
    "@types/connect-mongo": "^0.0.42",
    "@types/dotenv": "^6.1.1",
    "@types/express": "^4.17.0",
    "@types/hbs": "^4.0.1",
    "@types/jquery": "^3.3.30",
    "@types/mongoose": "^5.5.6",
    "@types/node": "^12.0.10",
    "@types/shelljs": "^0.8.5",
    "@types/winston": "^2.4.4",
    "concurrently": "^4.1.0",
    "node-sass": "^4.11.0",
    "nodemon": "^1.19.1",
    "shelljs": "^0.8.3",
    "ts-node": "^8.3.0",
    "tslint": "^5.18.0",
    "typescript": "^3.5.2"
  },
  "engines": {
    "node": "10.x"
  }
}
